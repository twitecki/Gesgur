{
  "name": "gesgur",
  "description": "Interactive imgur mode for the myo arm band",
  "version": "2.0",
  "content_scripts": [
    {
      "matches": ["http://imgur.com/gallery/*"],
      "css": ["mystyles.css", "bootstrap.css"],
      "js": ["jquery.min.js", "functions.js", "toggling.js", "myoimgur.js"],
      "run_at": "document_end"
    }
  ],
  "icons": { "48": "gesgurlogo.png"},
  "manifest_version": 2
}